[Unit]
Description = isucon6 qualifier sub application in PHP

[Service]
WorkingDirectory=/home/isucon/webapp/php
EnvironmentFile=/home/isucon/env.sh

ExecStart = /home/isucon/.local/php/sbin/php-fpm --fpm-config /home/isucon/.local/php/etc/isutar.php-fpm.conf

Restart   = always
Type      = simple
User      = isucon
Group     = isucon

[Install]
WantedBy = multi-user.target
